<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon.CargarCinta">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="Tab1" label="Recursos Humanos">
				<group id="Group1" label="Gestor">
					<button id="Button1" label="Principal" image="store_32"  size="large"  getEnabled="Ribbon.DesactivarBoton1" onAction="Boton1" />	
				</group>
				<group id="Group2" label="Datos">		
					<button id="Button2" label="Personal" image="contact_32"  size="large"  getEnabled="Ribbon.DesactivarBoton2" onAction="Boton2" />		
					<menu id="Menu1" label="Horas" size="large" itemSize="large" image = "clock_32" >
						<button id="Button25" label="Registrar Hora" image = "time_32" onAction="Boton25"/>
						<button id="Button26" label="Importar Datos" image = "memory_32" onAction="Boton26"/>
					</menu>	
				</group>
				<group id="Group3" label="Ingresos / Egresos">
					<button id="Button27" label="Comision" image = "bill_32" size="large"  onAction="Boton27"/>
					<button id="Button7" label="Ajuste" image="calculator_32" size="large"  getEnabled="Ribbon.DesactivarBoton7" onAction="Boton7" />
					<button id="Button4" label="Viatico" image="bag_32" size="large"  getEnabled="Ribbon.DesactivarBoton4" onAction="Boton4" />		
				</group>
				<group id="Group4" label="Jornada">	
					<button id="Button8" label="Jornada" image="bar_32" size="large"  getEnabled="Ribbon.DesactivarBoton8" onAction="Boton8" />
					<button id="Button9" label="Calendario" image="calendar_32" size="large"  getEnabled="Ribbon.DesactivarBoton9" onAction="Boton9" />
					<button id="Button39" label="Incapacidad" image = "rest_32" size="large"  onAction="Boton39"/>	
				</group>
				<group id="Group5" label="Reportes">						
					<button id="Button28" label="Planilla" image = "test_32"  size="large"   onAction="Boton28"/>
					<button id="Button29" label="Colillas" image = "stub_32"  size="large" onAction="Boton29"/>	
				</group>
				<group id="Group6" label="Especial">
				      <button id="Button14" label="Exonerar" image="topic_32"  size="large"  getEnabled="Ribbon.DesactivarBoton14" onAction="Boton14" />
					<button id="Button15" label="Anular" image="ticket_32"  size="large"  getEnabled="Ribbon.DesactivarBoton15" onAction="Boton15" />	
				</group>
				<group id="Group7" label="Usuarios">
					<button id="Button16" label="Nuevo" image="add_user_32"  size="large"  getEnabled="Ribbon.DesactivarBoton16" onAction="Boton16" />		
					<button id="Button17" label="Eliminar" image="delete_user_32"  size="large"  getEnabled="Ribbon.DesactivarBoton17" onAction="Boton17" />	
					<button id="Button18" label="Permisos" image="access_user_32"  size="large"  getEnabled="Ribbon.DesactivarBoton18" onAction="Boton18" />
				</group>
				<group id="Group8" label=" ">
					<button id="Button19" label="Cambiar" image="key_32"  size="large"  getEnabled="Ribbon.DesactivarBoton19" onAction="Boton19" />		
				</group>
				<group id="Group9" label=" ">
					<button id="Button20" label="Guardar " image="save_32"  size="large"  getEnabled="Ribbon.DesactivarBoton20" onAction="Boton20" />		
				</group>			
			</tab>
		</tabs>
	</ribbon>
</customUI>
